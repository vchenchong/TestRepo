        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".overscroll.OverScrollTestActivity">
        </activity>

        <activity android:name=".nestedscroll.NestedScrollTestActivity">
        </activity>

        <activity android:name=".multitouch.MultiTouchTestActivity">
        </activity>

        <activity android:name=".ime.IMETestActivity"
