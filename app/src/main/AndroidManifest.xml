            >
        </activity>

        <activity android:name=".expandgridview.ExpandViewTestActivity">
        </activity>

        <activity android:name=".expandgridview.EqualDivisionGridTestActivity">
        </activity>

        <activity android:name=".phoneview.PhoneViewTestActivity">
        </activity>

        <activity android:name=".dialog.DialogTestActivity">
        </activity>

        <activity android:name=".tabhost.TabHostActivity">
        </activity>

        <activity android:name=".progressbar.ProgressBarTestActivity">
        </activity>

        <activity android:name=".activity.transparent.TransparentTestActivity"
            android:theme="@style/TransparentTheme"
            >
        </activity>

    </application>

</manifest>